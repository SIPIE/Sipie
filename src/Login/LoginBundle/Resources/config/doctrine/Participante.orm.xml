<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Login\LoginBundle\Entity\Participante" table="participante">
    <id name="rut" type="string" column="rut" length="20">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombres" type="string" column="nombres" length="120" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidos" type="string" column="apellidos" length="120" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="120" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sexo" type="string" column="sexo" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechanacimiento" type="string" column="fechanacimiento" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lateralidad" type="string" column="lateralidad" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
